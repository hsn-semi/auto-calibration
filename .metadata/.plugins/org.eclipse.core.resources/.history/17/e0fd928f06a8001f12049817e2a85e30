			 //Spacer: 3x (UB, 5P, Temperature)
			 //strcat(strTemp,"0;0;0;");

			 //PC-Current
			 //Ambient Light
			 strcat(strTemp, "0;0;0;0;");




		}

		/* Log-Data CAN XY-Table */
		if(LogActive == 22)
